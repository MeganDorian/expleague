<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<VBox fx:id="root" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.expleague.expert.forms.PatchController">
  <padding><Insets top="2" left="2" bottom="2" right="2"/></padding>
  <VBox maxHeight="110" maxWidth="110" styleClass="patch" stylesheets="/css/patches.css">
    <HBox><Label layoutY="2" fx:id="label"/><ImageView fitHeight="13" preserveRatio="true" onMouseClicked="#delete"><Image url="/images/cross.png"/></ImageView></HBox>
    <TextArea fx:id="text" editable="false" wrapText="true"/>
  </VBox>
</VBox>